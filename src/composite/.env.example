# MCP Proxy Server - Environment Variables
# Copy this file to .env and fill in your values

# =============================================================================
# DIFY CONFIGURATION (required if dify backend is enabled)
# =============================================================================
DIFY_API_KEY=your_dify_api_key_here
DIFY_BASE_URL=https://api.dify.ai/v1
DIFY_CONSOLE_API_KEY=your_dify_console_api_key_here
DIFY_CONSOLE_BASE_URL=https://api.dify.ai

# =============================================================================
# BROWSER CONFIGURATION (optional)
# =============================================================================
BROWSER_TIMEOUT=30000
NAVIGATION_TIMEOUT=60000

# =============================================================================
# VECTORSTORE CONFIGURATION (required if vectorstore backend is enabled)
# =============================================================================
OPENAI_API_KEY=your_openai_api_key_here
